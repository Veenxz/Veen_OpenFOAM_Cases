    #include "forceCoeffs"
    
    #includeFunc fieldAverage(U, p, prime2Mean = yes)
    
    sixDoFRigidBodyState
    {
        type           sixDoFRigidBodyState;
        libs           ("libsixDoFRigidBodyState.so");
        angleFormat    degrees;
    }
}

