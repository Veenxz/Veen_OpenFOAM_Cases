FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10000;

deltaT          0.5;    //recommend time step is between 0.25 to 1, 
                         //other value may cause converge problem and residual oscillation

writeControl    adjustableRunTime;

writeInterval   500;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    
    //#include "include/probes"
    //#include "include/fieldAverage"
    #includeFunc residuals
    #includeFunc Q
    //#includeFunc Lambda2
}


// ************************************************************************* //
